{
    "name": "vatradar/slurp",
    "description": "Get all the data from vatsim.",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "VATRadar Devs",
            "email": "dev@vatradar.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Slurp\\": "src/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.0",
        "spaze/phpstan-disallowed-calls": "^2.11"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Slurp": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "react/event-loop": "^1.3",
        "vatradar/vatsimclient": "^0.6",
        "vatradar/env": "^0.6",
        "vatradar/dataobjects": "^0.6",
        "bunny/bunny": "@dev",
        "react/promise": "^2.9"
    },
    "config": {
        "bin-dir": "vbin",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.6-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://vatradar.repo.repman.io"
        }
    ]
}
